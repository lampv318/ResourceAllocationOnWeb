<div class="row" >
  <div class="col-lg-offset-4 col-lg-4 col-sm-4">
    <div class="circle-tile">
      <a href="{{url('admin/projects')}}">
        <div class="circle-tile-heading green">
          <i class="fa fa-archive fa-fw fa-3x"></i>
        </div>
      </a>
      <div class="circle-tile-content green">
        <div class="circle-tile-description text-faded">
          Project 
        </div>
        <div class="circle-tile-number text-faded">
          <%= @project.id %>
          <span id="sparklineA"></span>
        </div>
        <%= link_to run_project_path(duration: @project.duration, dependence: @project.dependence,
          matrix_experiment: @project.matrix_experiment, level_experience: @project.level_experience,
          weighted_of_fitness_1: @project.weighted_of_fitness_1, weighted_of_fitness_2: 
          @project.weighted_of_fitness_2, project_id: @project.id ),
          method: "POST", class: "circle-tile-footer" do %>
          Run Project with GA <i class="fa fa-chevron-circle-right"></i>
        <% end %>
      </div>
    </div>
  </div>
</div>
<div class = "container"> 
  <div class="row">
    <div class="col-sm-3">
      <div id = "page-wrapper1">
      <h5>List of duration</h5>
      <%= break_the_lines( @project.duration) %>
      </div>
    </div>
    <div class="col-sm-3">
      <div id = "page-wrapper1">
        <h5>List of Depedences </h5>
        <%= break_the_lines( @project.dependence) %>
      </div>
    </div>
    <div class="col-sm-3">
      <div id = "page-wrapper1">
        <h5>Matrix Experience</h5>
        <%= break_the_lines( @project.matrix_experiment) %>
      </div>
    </div>
    <div class="col-sm-3">
      <div id = "page-wrapper1">
      <h5>Level Experiencee</h5>
      <%= break_the_lines( @project.level_experience) %>
      </div>
    </div>

  </div>
</div>
<!-- <li><%= link_to "Listf Depedences ", "#"%></li>
<li><%= link_to "Matrix Experience ", "#"%></li><br> -->
<div class="row">
  <div class="col-md-12">
    <div class="panel panel-success">
      <div class="panel-heading">
        <h3 class="panel-title project" value="{{$project_id}}">Project - <%= @project.name  %></h3>
      </div>
      <div class="panel-body">                    
        <div class="col-md-12">
          <table class="table table-striped custab">
            <thead>
             <tr>
                <th>Phase</th>
                <th>Time</th>
                <th>Weighted of Fitness 1 </th>
                <th>Weighted of Fitness 2</th>
            </tr>
          </thead>
          <tr>                           
            <td>100</td>
            <td>24.98s</td>
            <td><%= @project.weighted_of_fitness_2 %></td>
            <td><%= @project.weighted_of_fitness_1 %></td>                            
          </tr>   
          </table>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="row">
	<div class="col-md-12">
		<div class="panel panel-success">
			<div class="panel-heading">
				<h3 class="panel-title">List Suggestion</h3>
			</div>
			<div class="panel-body">
				<div class="col-md-12">
				  <table class="table table-striped custab">
				    <thead>
				    	<tr>
					      <th>Number</th>                                
					      <th>Number of Phase</th>
					      <th>Time</th>
					      <th>Genes</th>
					      <th>Fitness</th>
					      <th class="text-center">Action</th>
				    	</tr>
				  	</thead>
				  	<% if @suggestion %>
				  	<% @suggestion.each do |suggestion| %>
					  <tr>                           
				      <td>1</td>
				      <td id="phase-{{$s['number']}}">500</td>
				      <td id="time-{{$s['number']}}">1.01</td>
				      <td id="payment-activity-{{$s['number']}}"><%= suggestion.genes %></td>
				      <td id="fit-{{$s['number']}}"><%= suggestion.fitness %></td>                            
				      <td class="text-center">
				          <button type="button" class="btn btn-danger">
				            <i class="fa fa-trash"></i>
				          </button>                               
				      </td>
					  </tr>
					  <% end %>
					  <% end %>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>


