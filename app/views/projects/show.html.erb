<div class="row" >
  <div class="col-lg-offset-4 col-lg-4 col-sm-4">
    <div class="circle-tile">
      <a href="{{url('admin/projects')}}">
        <div class="circle-tile-heading green">
          <i class="fa fa-archive fa-fw fa-3x"></i>
        </div>
      </a>
      <div class="circle-tile-content green">
        <div class="circle-tile-description text-faded">
          Project 
        </div>
        <div class="circle-tile-number text-faded">
          <%= @project.id %>
          <span id="sparklineA"></span>
        </div>
        <%= link_to run_project_path(duration: @project.duration, dependence: @project.dependence,
          matrix_experiment: @project.matrix_experiment, level_experience: @project.level_experience,
          weighted_of_fitness_1: @project.weighted_of_fitness_1, weighted_of_fitness_2: @project.weighted_of_fitness_2 ),
          method: "POST", class: "circle-tile-footer" do %>
          Run Project with GA <i class="fa fa-chevron-circle-right"></i>
        <% end %>
      </div>
    </div>
  </div>
</div>
<div class = "container"> 
  <div class="row">
    <div class="col-sm-3">
      <div id = "page-wrapper1">
      <h5>List of duration</h5>
      <%= break_the_lines( @project.duration) %>
      </div>
    </div>
    <div class="col-sm-3">
      <div id = "page-wrapper1">
        <h5>List of Depedences </h5>
        <%= break_the_lines( @project.dependence) %>
      </div>
    </div>
    <div class="col-sm-3">
      <div id = "page-wrapper1">
        <h5>Matrix Experience</h5>
        <%= break_the_lines( @project.matrix_experiment) %>
      </div>
    </div>
    <div class="col-sm-3">
      <div id = "page-wrapper1">
      <h5>Level Experiencee</h5>
      <%= break_the_lines( @project.level_experience) %>
      </div>
    </div>

  </div>
</div>
